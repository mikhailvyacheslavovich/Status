<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ru.inobitec.status.repository.StatusMapper">

    <resultMap id="Status" type="ru.inobitec.taskone.model.OrderSessionEntity">
        <result property="id" column="id"/>
        <result property="statusName" column="status_name"/>
    </resultMap>

    <insert id="changeStatus" parameterType="ru.inobitec.status.model.StatusEntity">
        <selectKey keyProperty="id" keyColumn="id" resultType="long" order="BEFORE">SELECT nextval('STATUS_SEQ')
        </selectKey>
        INSERT INTO status (id, status_name) VALUES (#{id}, #{statusName});
    </insert>

</mapper>